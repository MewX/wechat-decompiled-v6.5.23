<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/c6" />
            <solid android:color="@color/hx" />
            <stroke android:width="@dimen/c7" android:color="@color/i0" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/c6" />
            <solid android:color="@color/hz" />
            <stroke android:width="@dimen/c7" android:color="@color/i2" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/c6" />
            <solid android:color="@color/hy" />
            <stroke android:width="@dimen/c7" android:color="@color/i1" />
        </shape>
    </item>
</selector>