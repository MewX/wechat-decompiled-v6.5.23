<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/c6" />
            <solid android:color="@color/pt" />
            <stroke android:width="@dimen/c7" android:color="#33000000" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/c6" />
            <solid android:color="@color/pt" />
            <stroke android:width="@dimen/c7" android:color="#990692dd" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/c6" />
            <solid android:color="@color/pt" />
            <stroke android:width="@dimen/c7" android:color="#ff0692dd" />
        </shape>
    </item>
</selector>